import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        System.out.println("enter the number:");
        for(int i=1;i<=10;i++){
             System.out.println("Multiplication Table of " + n);
        System.out.println(n+ "*" +i+ "=" +(n*i));
        }
    }
}
