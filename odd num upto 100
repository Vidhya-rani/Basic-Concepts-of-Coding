import java.util.*;
public class odd_num_upto100 {
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.println("odd number from 1 to 100");
        for(int i=1;i<=100;i++) {
            if (i % 2 != 0) {
                System.out.println(i);
            }
        }
    }
}
