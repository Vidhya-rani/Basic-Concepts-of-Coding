import java.util.*;
public class positive_or_negative {
    public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the number:");
        int n=sc.nextInt();
        if(n>=0){
            System.out.println("positive");
        }
        else{
            System.out.println("negative");
        }
    }
}
