import java.util.Scanner;

public class SumofDigit {
    public static int getsum(int n){
        int sum=0;
      while(n!=0){
            int digit=n%10;
            sum=sum+digit;
            n=n/10;
        }
        return sum;
    }
    static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the Elements:");
        int n=sc.nextInt();
        int result=getsum(n);
        System.out.println(result);
    }
}
