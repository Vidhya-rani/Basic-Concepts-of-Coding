import java.util.*;
public class UpperCase_LowerCase_Digit_Special_char {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter the Character:");
        char ch=sc.next().charAt(0);
        if(ch>='A' && ch<='Z'){
            System.out.println("The character is Uppercase Letters:"+ ch);       
        }
        else  if( ch>='a' && ch<='z'){
            System.out.println(("the character is Lowercase Letters:"+ ch));
        }
        else if (ch>='0' && ch<='9'){
            System.out.println("The character is Digit:"+ ch);
        }
        else{
            System.out.println("The character is Special Character:"+ ch);
        }

    }
}
